{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": ["error", {"argsIgnorePattern": "^_"}],
    "no-console": ["off"],
    "global-require": ["off"]
  }
}
